SELECT  'Player_Name'
       ,  'Runs'
       ,  'Wickets'
       
  FROM     Team_Chennai2
UNION
  SELECT  'Player_Name'
       ,  'Runs'
       ,  'Wickets'
  FROM   Team_Chennai2
ORDER BY  1;
SELECT * FROM Team_Chennai2;
CREATE temporary TABLE allthrees as
select Player_Name, max(Runs) as Runs
from Team_Mumbai2
union all
select Player_Name, max(Runs) as Runs
from Team_Pune2
union all
select Player_Name, max(Runs) as Runs
from Team_Chennai2;
select Player_Name from allthrees where Runs=(select max(Runs) from allthrees);


CREATE temporary TABLE allteams as
select Player_Name, max(Wickets) as Wickets
from Team_Mumbai2
union all
select Player_Name, max(Wickets) as Wickets
from Team_Pune2
union all
select Player_Name, max(Wickets) as Wickets
from Team_Chennai2;
select Player_Name from allteams where Wickets=(select max(Wickets) from allteams);

SELECT Player_No,Player_Name, Runs, Wickets FROM Team_Mumbai
UNION ALL
SELECT Player_No,Player_Name, Runs, Wickets FROM Team_Mumbai2;


SELECT Player_No,Player_Name, Runs, Wickets FROM Team_Pune
UNION ALL
SELECT Player_No,Player_Name, Runs, Wickets FROM Team_Pune2;

SELECT Player_No,Player_Name, Runs, Wickets FROM Team_Chennai
UNION ALL
SELECT Player_No,Player_Name, Runs, Wickets FROM Team_Chennai2;


Select avg(Runs) from Team_Mumbai;
Select avg(Runs) from Team_Pune;
Select avg(Runs) from Team_Chennai;








