/* Display all the records from the table */
SELECT  'Player_Name'
       ,  'Runs'
       ,  'Wickets'
       
  FROM     Team_Chennai
UNION
  SELECT  'Player_Name'
       ,  'Runs'
       ,  'Wickets'
  FROM   Team_Chennai
ORDER BY  1;
SELECT * FROM Team_Chennai;
CREATE temporary TABLE allthree as
select Player_Name, max(Runs) as Runs
from Team_Mumbai
union all
select Player_Name, max(Runs) as Runs
from Team_Pune
union all
select Player_Name, max(Runs) as Runs
from Team_Chennai;
select Player_Name from allthree where Runs=(select max(Runs) from allthree);


CREATE temporary TABLE allteam as
select Player_Name, max(Wickets) as Wickets
from Team_Mumbai
union all
select Player_Name, max(Wickets) as Wickets
from Team_Pune
union all
select Player_Name, max(Wickets) as Wickets
from Team_Chennai;
select Player_Name from allteam where Wickets=(select max(Wickets) from allteam);

CREATE TABLE IPLTWO(ID integer PRIMARY KEY, League_team text, Players integer,Matches integer);

/* Create few records in this table */
INSERT INTO IPLTWO VALUES(1,'Team Mumbai','11','5');
INSERT INTO IPLTWO VALUES(2,'Team Pune','11','5');
INSERT INTO IPLTWO VALUES(3,'Team Chennai','11','5');
COMMIT;
SELECT * FROM IPLTWO;
