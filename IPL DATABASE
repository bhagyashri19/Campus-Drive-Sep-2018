BEGIN TRANSACTION;

/* Create a table called IPLONE*/
CREATE TABLE IPLONE(ID integer PRIMARY KEY, League_team text, Players integer,Matches integer);

/* Create few records in this table */
INSERT INTO IPLONE VALUES(1,'Team Mumbai','11','5');
INSERT INTO IPLONE VALUES(2,'Team Pune','11','5');
INSERT INTO IPLONE VALUES(3,'Team Chennai','11','5');

SELECT * FROM IPLONE;
CREATE TABLE Team_Mumbai(Player_No integer, Player_Name text, Runs integer, Wickets integer);
INSERT INTO Team_Mumbai VALUES(11,'Aakash','24','1');
INSERT INTO Team_Mumbai VALUES(12,'Akshay','34','2');
INSERT INTO Team_Mumbai VALUES(13,'Aman','45','3');
INSERT INTO Team_Mumbai VALUES(14,'Bhavesh','56','5');
INSERT INTO Team_Mumbai VALUES(15,'Chaitanya','56','6');
INSERT INTO Team_Mumbai VALUES(16,'Pratik','89','7');
INSERT INTO Team_Mumbai VALUES(17,'Chintu','78','8');
INSERT INTO Team_Mumbai VALUES(18,'PInku','24','9');
INSERT INTO Team_Mumbai VALUES(20,'Gopal','55','10');
INSERT INTO Team_Mumbai VALUES(21,'Tarun','11','11');
INSERT INTO Team_Mumbai VALUES(22,'Rahul','0','12');
  SELECT  
          'Player_Name'
       ,  'Runs'
       ,  'Wickets'
       
  FROM     Team_Mumbai
UNION
  SELECT 'Player_Name'
       ,  'Runs'
       ,  'Wickets'
  FROM   Team_Mumbai
ORDER BY  1;
SELECT * FROM Team_Mumbai;
CREATE TABLE Team_Pune(Player_No integer, Player_Name text, Runs integer, Wickets integer);

INSERT INTO Team_Pune VALUES(23,'a','24','1');
