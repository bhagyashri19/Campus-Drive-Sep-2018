       ,  'Runs'
       ,  'Wickets'
       
  FROM     Team_Pune
UNION
  SELECT 'Player_Name'
       ,  'Runs'
       ,  'Wickets'
  FROM   Team_Pune
ORDER BY  1;
SELECT * FROM Team_Pune;
CREATE TABLE Team_Chennai(Player_No integer, Player_Name text, Runs integer, Wickets integer);
INSERT INTO Team_Chennai VALUES(34,'l','24','1');
